<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.parkingmanagement">

    <application
        android:name=".ParkingManagementApp"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ParkingManagement">
        <activity
            android:name=".presentation.ui.entrance.EntranceActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".presentation.ui.newparkingspace.NewParkingSpaceActivity" android:noHistory="true"/>
        <activity android:name=".presentation.ui.home.HomeActivity"/>
        <activity android:name=".presentation.ui.allparking.AllParkingActivity" android:noHistory="true"/>
        <activity android:name=".presentation.ui.allreservation.AllReservationActivity" android:noHistory="true"/>
        <activity android:name=".presentation.ui.newparking.NewParkingActivity" android:noHistory="true"/>
        <activity android:name=".presentation.ui.newreservation.NewReservationActivity" android:noHistory="true"/>
    </application>

</manifest>
